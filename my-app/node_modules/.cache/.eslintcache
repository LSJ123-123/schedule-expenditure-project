alert(\"2 입니다.\");\r\n      }\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <form>\r\n        <input\r\n          type=\"text\"\r\n          name=\"id\"\r\n          placeholder=\"id\"\r\n          value={state.id}\r\n          onChange={handleChange}\r\n        ></input>\r\n\r\n        <button type=\"submit\" onClick={pre_id_Check}>\r\n          전송\r\n        </button>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\nexport default Login;\r\n","C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Create_ID_Duplicate.js",[],[],"C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Create_PW_Rule_check.js",[],[],"C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Create_User_Data.js",[],[],"C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Find_ID_Email.js",[],[],"C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Find_PW_ID_Phone.js",[],[],[],"C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Login_ID_PW.js",[],[],{"ruleId":"79","severity":1,"message":"80","line":16,"column":57,"nodeType":"81","messageId":"82","endLine":16,"endColumn":68},{"ruleId":"79","severity":1,"message":"83","line":17,"column":57,"nodeType":"81","messageId":"82","endLine":17,"endColumn":68},{"ruleId":"84","severity":1,"message":"85","line":1,"column":27,"nodeType":"86","messageId":"87","endLine":1,"endColumn":36},"react/jsx-pascal-case","Imported JSX component Find_id must be in PascalCase or SCREAMING_SNAKE_CASE","JSXOpeningElement","usePascalOrSnakeCase","Imported JSX component Find_pw must be in PascalCase or SCREAMING_SNAKE_CASE","@typescript-eslint/no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar"]