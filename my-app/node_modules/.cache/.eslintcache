n pre_id_Check() {\r\n    if (state.id === \"\") {\r\n      alert(\"아이디를 입력해주세요.\");\r\n    } else {\r\n      var test = await rec_msg();\r\n\r\n      alert(test);\r\n      if (test === 0) {\r\n        alert(\"0 입니다.\");\r\n      } else if (test === 1) {\r\n        alert(\"1 입니다.\");\r\n      } else {\r\n        alert(\"2 입니다.\");\r\n      }\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <form>\r\n        <input\r\n          type=\"text\"\r\n          name=\"id\"\r\n          placeholder=\"id\"\r\n          value={state.id}\r\n          onChange={handleChange}\r\n        ></input>\r\n\r\n        <button type=\"submit\" onClick={pre_id_Check}>\r\n          전송\r\n        </button>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\nexport default Login;\r\n","C:\\Users\\gkscj\\Desktop\\schedule-expenditure-project\\my-app\\src\\Components\\Function\\Create_ID_Duplicate.js",[],[],{"ruleId":"54","severity":1,"message":"55","line":16,"column":57,"nodeType":"56","messageId":"57","endLine":16,"endColumn":68},{"ruleId":"54","severity":1,"message":"58","line":17,"column":57,"nodeType":"56","messageId":"57","endLine":17,"endColumn":68},{"ruleId":"59","severity":1,"message":"60","line":22,"column":9,"nodeType":"61","messageId":"62","endLine":22,"endColumn":25},{"ruleId":"59","severity":1,"message":"63","line":1,"column":27,"nodeType":"61","messageId":"62","endLine":1,"endColumn":36},"react/jsx-pascal-case","Imported JSX component Find_id must be in PascalCase or SCREAMING_SNAKE_CASE","JSXOpeningElement","usePascalOrSnakeCase","Imported JSX component Find_pw must be in PascalCase or SCREAMING_SNAKE_CASE","@typescript-eslint/no-unused-vars","'navigateToCreate' is assigned a value but never used.","Identifier","unusedVar","'useEffect' is defined but never used."]